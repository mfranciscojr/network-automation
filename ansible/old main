- name: show access-list interfaces
  ios_command:
    commands:
      - show ip access-list vty-acl
  register: result

- name: Show result of commands
  debug:
    var: result.stdout_lines
- name: backup cisco config
  ios_config:
    backup: yes
    backup_options:
      dir_path: ./misc/backup_configs/cisco



no ip access-list standard vty-acl
!
ip access-list standard vty-acl
!
{% for cisco_vty_acl_host in cisco_vty_acl_host %}
permit host {{cisco_vty_acl_host}} log
{% endfor %}
!